	<select id="queryById" parameterType="ProjectoneBean" resultType="ProjectoneBean">
		select * from projectone where proid=#{proid}
	</select>


	<insert id="add" parameterType="ProjectoneBean"
		useGeneratedKeys="true" keyProperty="proid">
		insert into projectone
		(proname,promonitorStr,prostructure,proarea,proprice,prostate,proprocess,
		proslice,proaddress,proconfactoryid,proconfactoryStr,probemanageid,probemanageStr,
		proconfactoryid1,proconfactoryStr1,probemanageid1,probemanageStr1,
		proconfactoryid2,proconfactoryStr2,probemanageid2,probemanageStr2,towerStr,elevatorStr,mateeleStr,otherdeviceStr,
		prostarttime,proenforce,proevaluateself,proevaluateall,proevaluatelevel,proextrainfo,
		prouserid,prousername,prodate)
		values
		(#{proname},#{promonitorStr},#{prostructure},#{proarea},#{proprice},#{prostate},
		#{proprocess},#{proslice},#{proaddress},
		#{proconfactoryid},#{proconfactoryStr},#{probemanageid},#{probemanageStr},
		#{proconfactoryid1},#{proconfactoryStr1},#{probemanageid1},#{probemanageStr1},
		#{proconfactoryid2},#{proconfactoryStr2},#{probemanageid2},#{probemanageStr2},
		#{towerStr},#{elevatorStr},#{mateeleStr},#{otherdeviceStr},#{prostarttime},#{proenforce},
		#{proevaluateself},#{proevaluateall},#{proevaluatelevel},#{proextrainfo},
		#{prouserid},#{prousername},#{prodate})
	</insert>
	<update id="update" parameterType="ProjectoneBean">
