		prostate=#{prostate},proprocess=#{proprocess},proslice=#{proslice},proaddress=#{proaddress},
		proconfactoryid=#{proconfactoryid},proconfactoryStr=#{proconfactoryStr},probemanageid=#{probemanageid},probemanageStr=#{probemanageStr},
		proconfactoryid1=#{proconfactoryid1},proconfactoryStr1=#{proconfactoryStr1},probemanageid1=#{probemanageid1},probemanageStr1=#{probemanageStr1},
		proconfactoryid2=#{proconfactoryid2},proconfactoryStr2=#{proconfactoryStr2},probemanageid2=#{probemanageid2},probemanageStr2=#{probemanageStr2},
		towerStr=#{towerStr},elevatorStr=#{elevatorStr},
		mateeleStr=#{mateeleStr},otherdeviceStr=#{otherdeviceStr},prostarttime=#{prostarttime},proenforce=#{proenforce},
		proevaluateself=#{proevaluateself},proevaluateall=#{proevaluateall},proevaluatelevel=#{proevaluatelevel},
		proextrainfo=#{proextrainfo} where proid=#{proid}
	</update>
